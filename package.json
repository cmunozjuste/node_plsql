    "grunt-contrib-watch": ">=0.6.1",
    "grunt-jscs": ">=0.8.1",
    "grunt-jsonlint": ">=1.0.4",
    "grunt-mocha-istanbul": ">=2.2.0",
    "grunt-mocha-test": ">=0.12.4",
    "mocha": ">=2.0.1",
    "supertest": ">=0.15.0"
  }
