{
  "name": "node_plsql",
  "version": "0.1.0",
  "author": "Dieter Oberkofler <doberkofler.github@gmail.com>",
  "description": "The Node.js PL/SQL Gateway for Oracle (nodejs, plsql)",
  "bin": {
    "node_plsql": "./bin/node_plsql.js"
  },
  "preferGlobal": true,
  "main": "./lib/api.js",
  "keywords": [
    "plsql",
    "pl/sql",
    "node",
    "nodejs",
    "listener",
    "server",
    "apex",
    "mod_plsql",
    "ohs",
    "database",
    "db",
    "oracle"
  ],
  "homepage": "https://github.com/doberkofler/node_plsql",
  "license": "MIT",
  "engines": {
    "node": ">=0.10"
  },
  "maintainers": [
    {
      "name": "Dieter Oberkofler",
      "email": "doberkofler.github@gmail.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/doberkofler/node_plsql/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/doberkofler/node_plsql.git"
  },
  "files": [
    "bin",
    "conf",
    "demo",
    "lib",
    "sql",
    "src",
    "test",
    ".jshintrc",
    "AUTHORS",
    "binding.gyp",
    "build_environment.cmd",
    "build_environment.sh",
    "HISTORY.md",
    "INSTALL.md",
    "LICENSE",
    "Makefile.js",
    "package.json",
    "README.md",
    "RELEASE-GOALS.md"
  ],
  "scripts": {
    "install": "node-pre-gyp install --fallback-to-build",
    "lint": "grunt lint",
    "test": "grunt test"
  },
  "binary": {
    "module_name" : "oracleBindings",
    "module_path" : "./build/{module_name}/v{version}/{configuration}/{node_abi}-{platform}-{arch}/",
    "remote_path" : "./{module_name}/v{version}/{configuration}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz",
    "host" : "https://nodeplsql.s3.amazonaws.com"
  },
  "dependencies": {
    "body-parser": "^1.10.0",
    "compression": "^1.2.2",
    "connect-multiparty": "^1.2.5",
    "cookie-parser": "^1.3.3",
