    "ms": "^0.7.0",
    "nan": "^1.4.1",
    "node-pre-gyp": "^0.6.1",
    "nopt": "^3.0.1",
    "serve-static": "^1.7.1",
    "strip-json-comments": "^1.0.2",
    "tough-cookie": "^0.12.1",
    "underscore": "^1.7.0"
  },
  "bundledDependencies": ["node-pre-gyp"],
  "devDependencies": {
    "aws-sdk": ">=2.1.4",
    "chai": ">=1.10.0",
    "grunt": ">=0.4.5",
    "grunt-contrib-clean": ">=0.6.0",
